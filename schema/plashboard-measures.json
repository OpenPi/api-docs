{
	"type": "object",
	"$schema": "http://json-schema.org/draft-03/schema",
	"id": "http://jsonschema.net",
	"required": true,
	"properties": {
		"measure_id": {
			"type": "integer",
			"description": "Measure ID",
			"required": true,
		},
		"hardware_id": {
			"type": "integer",
			"description": "Sensor ID",
			"required": true,
		},
		"name": {
			"type": "string",
			"description": "Sensor name",
			"required": true,
		},
		"value": {
			"type": "integer",
			"description": "Sensor value",
			"required": true
		},
		"timestamp": {
			"type": "Datetime",
			"description": "Datetime's Sensor value",
			"required": true
		}
	}
}